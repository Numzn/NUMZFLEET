# Traccar GPS Tracking Configuration
# Copy this file to .env and update with your actual values

# Traccar Server URL (without trailing slash)
# Use your local IP address to access from phone
VITE_TRACCAR_URL=http://10.91.5.242:8082

# Traccar Authentication (base64 encoded username:password)
# To generate: echo -n "username:password" | base64
# Example: admin:admin (default Traccar credentials)
VITE_TRACCAR_AUTH=YWRtaW46YWRtaW4=

# Traccar Server Configuration (REAL MODE ONLY)
# Set your actual Traccar server URL and credentials

# Firebase Configuration (if using Firebase)
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=your_app_id

